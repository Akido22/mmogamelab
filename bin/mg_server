#!/usr/bin/python2.6

from mg.srv.server import Server
from mg.srv.common import Instance
from concurrence import dispatch
import traceback

def main():
    try:
        inst = Instance()
        server = Server(inst)
        server.run()
    except RuntimeError as e:
        print e
        quit(1)
    except:
        traceback.print_exc()
        quit(1)

dispatch(main)

