#!/usr/bin/python2.6

import mg

def main():
    inst = mg.Instance("reload", "metagam")
    app = inst.int_app
    app.load([
        "mg.core.cluster.Cluster",
    ])
    app.call("cluster.query-services", "metagam", "/core/abort", call_int=True, verbose=True)
    app.call("cluster.query-services", "realplexor", "/core/abort", call_int=True, verbose=True)

mg.dispatch(main)
